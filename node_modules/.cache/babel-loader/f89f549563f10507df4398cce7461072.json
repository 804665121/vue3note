{"ast":null,"code":"import { reactive, computed } from \"@vue/reactivity\";\nexport default {\n  name: \"ChangeUser\",\n\n  setup() {\n    let person = reactive({\n      firstName: \"\",\n      lastName: \"\"\n    }); // 计算属性\n\n    let fullName = computed(() => {\n      return person.firstName + person.lastName;\n    });\n    return {\n      fullName,\n      person\n    };\n  }\n\n};","map":{"version":3,"mappings":"AASA,SAASA,QAAT,EAAmBC,QAAnB,QAAmC,iBAAnC;AACA,eAAe;AACbC,MAAI,EAAE,YADO;;AAEbC,OAAK,GAAG;AACN,QAAIC,MAAK,GAAIJ,QAAQ,CAAC;AACpBK,eAAS,EAAE,EADS;AAEpBC,cAAQ,EAAE;AAFU,KAAD,CAArB,CADM,CAMN;;AAEA,QAAIC,QAAO,GAAIN,QAAQ,CAAC,MAAM;AAC5B,aAAOG,MAAM,CAACC,SAAP,GAAmBD,MAAM,CAACE,QAAjC;AACD,KAFsB,CAAvB;AAIA,WAAO;AACLC,cADK;AAELH;AAFK,KAAP;AAID;;AAlBY,CAAf","names":["reactive","computed","name","setup","person","firstName","lastName","fullName"],"sourceRoot":"","sources":["/Users/liulu/刘璐/vue3/hello-vue3/src/components/ChangeUser.vue"],"sourcesContent":["<template>\n  <div><h1>计算属性的学习</h1></div>\n\n  <div>姓: <input v-model=\"person.firstName\" /></div>\n  <div>名: <input v-model=\"person.lastName\" /></div>\n  <p>{{ fullName }}</p>\n</template>\n\n<script>\nimport { reactive, computed } from \"@vue/reactivity\";\nexport default {\n  name: \"ChangeUser\",\n  setup() {\n    let person = reactive({\n      firstName: \"\",\n      lastName: \"\",\n    });\n\n    // 计算属性\n\n    let fullName = computed(() => {\n      return person.firstName + person.lastName;\n    });\n\n    return {\n      fullName,\n      person,\n    };\n  },\n};\n</script>\n\n<style>\n</style>"]},"metadata":{},"sourceType":"module"}