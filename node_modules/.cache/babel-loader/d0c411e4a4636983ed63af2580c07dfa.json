{"ast":null,"code":"import MenuList from \"@/common/MenuList.vue\";\nimport { useContentStore } from \"@/store/content\";\nimport { storeToRefs } from \"pinia\";\nexport default {\n  components: {\n    MenuList\n  },\n\n  setup() {\n    let info = useContentStore();\n\n    const changeSum = () => {\n      info.addSum(3);\n    };\n\n    let {\n      title,\n      sum\n    } = storeToRefs(info); //使用pinia的storeRefs 对数据进行解构 直接解构的数据是非响应式的\n\n    return {\n      title,\n      sum,\n      changeSum\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAyBA,OAAOA,QAAP,MAAqB,uBAArB;AAEA,SAASC,eAAT,QAAgC,iBAAhC;AACA,SAASC,WAAT,QAA4B,OAA5B;AACA,eAAe;AACbC,YAAU,EAAE;AAAEH;AAAF,GADC;;AAEbI,OAAK,GAAG;AACN,QAAIC,IAAG,GAAIJ,eAAe,EAA1B;;AAEA,UAAMK,SAAQ,GAAI,MAAM;AACtBD,UAAI,CAACE,MAAL,CAAY,CAAZ;AACD,KAFD;;AAGA,QAAI;AAAEC,WAAF;AAASC;AAAT,QAAiBP,WAAW,CAACG,IAAD,CAAhC,CANM,CAMkC;;AACxC,WAAO;AACLG,WADK;AAELC,SAFK;AAGLH;AAHK,KAAP;AAKD;;AAdY,CAAf","names":["MenuList","useContentStore","storeToRefs","components","setup","info","changeSum","addSum","title","sum"],"sourceRoot":"","sources":["/Users/liulu/刘璐/vue3/hello-vue3/src/view/Home.vue"],"sourcesContent":["<template>\n  <a-layout>\n    <a-layout-header> 这是头部信息 </a-layout-header>\n    <a-layout>\n      <a-layout-sider>\n        <MenuList></MenuList>\n      </a-layout-sider>\n      <a-layout-content>\n        <a-card>\n          <template v-slot:title>\n            {{ title }}\n            <div>读取pinia内部的方法:{{ sum }}</div>\n            <button @click=\"changeSum\">修改pinia里面的sum</button>\n          </template>\n          <div class=\"content\">\n            <router-view />\n          </div>\n        </a-card>\n      </a-layout-content>\n    </a-layout>\n    <a-layout-footer>Footer</a-layout-footer>\n  </a-layout>\n</template>\n\n<script>\nimport MenuList from \"@/common/MenuList.vue\";\n\nimport { useContentStore } from \"@/store/content\";\nimport { storeToRefs } from \"pinia\";\nexport default {\n  components: { MenuList },\n  setup() {\n    let info = useContentStore();\n\n    const changeSum = () => {\n      info.addSum(3);\n    };\n    let { title, sum } = storeToRefs(info); //使用pinia的storeRefs 对数据进行解构 直接解构的数据是非响应式的\n    return {\n      title,\n      sum,\n      changeSum,\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.ant-layout {\n  height: 100vh;\n}\n\n.ant-layout-header {\n  text-align: center;\n  background-color: #ccc;\n}\n.content {\n  background-color: #fff;\n  //   padding: 20px;\n}\n.ant-layout-footer {\n  background-color: #e4e4e4;\n}\n</style>"]},"metadata":{},"sourceType":"module"}