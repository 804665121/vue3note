{"ast":null,"code":"import { reactive, ref, toRaw, markRaw } from \"vue\";\nexport default {\n  setup() {\n    let person = reactive({\n      name: \"zangsan\",\n      age: 19,\n      job: {\n        j1: 20\n      }\n    });\n    let x = ref(0);\n\n    function showRaw() {\n      let p = toRaw(person);\n      console.log(p);\n    }\n\n    function addCar() {\n      let car = {\n        carName: \"OOOO\",\n        price: 400\n      };\n      person.car = car;\n    }\n\n    return {\n      addCar,\n      showRaw,\n      person,\n      x\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoBA,SAASA,QAAT,EAAmBC,GAAnB,EAAwBC,KAAxB,EAA+BC,OAA/B,QAA8C,KAA9C;AACA,eAAe;AACbC,OAAK,GAAG;AACN,QAAIC,MAAK,GAAIL,QAAQ,CAAC;AACpBM,UAAI,EAAE,SADc;AAEpBC,SAAG,EAAE,EAFe;AAGpBC,SAAG,EAAE;AACHC,UAAE,EAAE;AADD;AAHe,KAAD,CAArB;AAQA,QAAIC,IAAIT,GAAG,CAAC,CAAD,CAAX;;AAEA,aAASU,OAAT,GAAmB;AACjB,UAAIC,IAAIV,KAAK,CAACG,MAAD,CAAb;AACAQ,aAAO,CAACC,GAAR,CAAYF,CAAZ;AACF;;AAEA,aAASG,MAAT,GAAkB;AAChB,UAAIC,GAAE,GAAI;AAAEC,eAAO,EAAE,MAAX;AAAmBC,aAAK,EAAE;AAA1B,OAAV;AACAb,YAAM,CAACW,GAAP,GAAcA,GAAd;AACF;;AAEA,WAAO;AACLD,YADK;AAELJ,aAFK;AAGLN,YAHK;AAILK;AAJK,KAAP;AAMD;;AA5BY,CAAf","names":["reactive","ref","toRaw","markRaw","setup","person","name","age","job","j1","x","showRaw","p","console","log","addCar","car","carName","price"],"sourceRoot":"","sources":["/Users/liulu/刘璐/vue3/hello-vue3/src/components/ToRaw.vue"],"sourcesContent":["<template>\n  <div><h1>toRaw/markRaw</h1></div>\n  <div>这是x的值:{{ x }}</div>\n  <button @click=\"x++\">x++</button>\n  <hr />\n\n  <div>{{ person }}</div>\n  <div>名字:{{ person.name }}</div>\n  <div>年龄:{{ person.age }}</div>\n  <div>薪水:{{ person.job.j1 }}k</div>\n  <div v-if=\"person.car\">车辆信息:{{ person.car }}</div>\n  <button @click=\"person.job.j1++\">加薪</button>\n  <button @click=\"person.age++\">年龄++</button>\n  <button @click=\"showRaw\">还原最原始的person</button>\n  <button @click=\"addCar\">添加一辆车</button>\n\n  <button @click=\"person.car.price++\">修改车次价格</button>\n</template>\n\n<script>\nimport { reactive, ref, toRaw, markRaw } from \"vue\";\nexport default {\n  setup() {\n    let person = reactive({\n      name: \"zangsan\",\n      age: 19,\n      job: {\n        j1: 20,\n      },\n    });\n\n    let x = ref(0);\n\n    function showRaw() {\n      let p = toRaw(person);\n      console.log(p);\n    }\n\n    function addCar() {\n      let car = { carName: \"OOOO\", price: 400 };\n      person.car = (car);\n    }\n\n    return {\n      addCar,\n      showRaw,\n      person,\n      x,\n    };\n  },\n};\n</script>\n\n<style>\n</style>"]},"metadata":{},"sourceType":"module"}