{"ast":null,"code":"import { reactive, toRefs, shallowRef, shallowReactive, readonly, shallowReadonly } from \"vue\";\nexport default {\n  setup() {\n    // let person = shallowReactive({ //只处理对象第一层的数据浅响应式\n    let person = reactive({\n      //只处理基本数据类型的响应式 不进行对象的响应式\n      name: \"zangsan\",\n      age: 19,\n      job: {\n        j1: 20\n      }\n    });\n    let x = shallowRef(0); //只处理基本数据类型的响应式 不进行对象的响应式\n    //  person = readonly(person); //使用readonly对数据进行包装 使数据是只读的\n\n    person = shallowReadonly(person); //浅只读 深层次的可以更新\n\n    return {\n      person,\n      x\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiBA,SACEA,QADF,EAEEC,MAFF,EAGEC,UAHF,EAIEC,eAJF,EAKEC,QALF,EAMEC,eANF,QAOO,KAPP;AAQA,eAAe;AACbC,OAAK,GAAG;AACN;AACA,QAAIC,MAAK,GAAIP,QAAQ,CAAC;AACpB;AACAQ,UAAI,EAAE,SAFc;AAGpBC,SAAG,EAAE,EAHe;AAIpBC,SAAG,EAAE;AACHC,UAAE,EAAE;AADD;AAJe,KAAD,CAArB;AASA,QAAIC,IAAIV,UAAU,CAAC,CAAD,CAAlB,CAXM,CAWiB;AAEvB;;AACAK,UAAK,GAAIF,eAAe,CAACE,MAAD,CAAxB,CAdM,CAc4B;;AAElC,WAAO;AACLA,YADK;AAELK;AAFK,KAAP;AAID;;AArBY,CAAf","names":["reactive","toRefs","shallowRef","shallowReactive","readonly","shallowReadonly","setup","person","name","age","job","j1","x"],"sourceRoot":"","sources":["/Users/liulu/刘璐/vue3/hello-vue3/src/components/Other.vue"],"sourcesContent":["<template>\n  <div><h1>shallowReactive/shallowRef的使用</h1></div>\n  <div><h1>readonly/shallowReadonly</h1></div>\n  <div>这是x的值:{{ x }}</div>\n  <button @click=\"x++\">x++</button>\n  <hr />\n\n  <div>{{ person }}</div>\n  <div>名字:{{ person.name }}</div>\n  <div>年龄:{{ person.age }}</div>\n  <div>薪水:{{ person.job.j1 }}k</div>\n\n  <button @click=\"person.job.j1++\">加薪</button>\n  <button @click=\"person.age++\">年龄++</button>\n</template>\n\n<script>\nimport {\n  reactive,\n  toRefs,\n  shallowRef,\n  shallowReactive,\n  readonly,\n  shallowReadonly,\n} from \"vue\";\nexport default {\n  setup() {\n    // let person = shallowReactive({ //只处理对象第一层的数据浅响应式\n    let person = reactive({\n      //只处理基本数据类型的响应式 不进行对象的响应式\n      name: \"zangsan\",\n      age: 19,\n      job: {\n        j1: 20,\n      },\n    });\n\n    let x = shallowRef(0); //只处理基本数据类型的响应式 不进行对象的响应式\n\n    //  person = readonly(person); //使用readonly对数据进行包装 使数据是只读的\n    person = shallowReadonly(person); //浅只读 深层次的可以更新\n\n    return {\n      person,\n      x,\n    };\n  },\n};\n</script>\n\n<style>\n</style>"]},"metadata":{},"sourceType":"module"}