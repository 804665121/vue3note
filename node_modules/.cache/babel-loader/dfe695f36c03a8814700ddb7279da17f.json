{"ast":null,"code":"import { reactive, ref } from \"vue\";\nimport func from \"vue-editor-bridge\";\nexport default {\n  name: \"setup\",\n\n  setup() {\n    /**\n     * reactive 修改一个响应式对象 不处理基本数据类型\n     * reactive中定义的对象无需.value 可以直接取值\n     * ref 处理基本数据类型\n     * ref中的引用数据类型需要objName.value 取值 实际上ref中的对象处理也是求助了reactive中的proxy\n     */\n    // name job 都是引用对象\n    let num = ref(1);\n    let job = ref({\n      type: \"工程师\",\n      name: \"ui设计\"\n    });\n    let jobArr = ref([{\n      type: \"工程师\",\n      name: \"ui设计\"\n    }]);\n    let objArr = reactive({\n      work: \"上班了吗\",\n      age: 12\n    });\n\n    function add() {\n      num.value++;\n      job.value.type = \"技术员\";\n      job.value.name = \"电工\";\n      console.log(job);\n    }\n\n    function changeObj() {\n      objArr.age = 30;\n    }\n\n    return {\n      add,\n      num,\n      job,\n      jobArr,\n      objArr\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAWA,SAASA,QAAT,EAAmBC,GAAnB,QAA8B,KAA9B;AACA,OAAOC,IAAP,MAAiB,mBAAjB;AACA,eAAe;AACbC,MAAI,EAAE,OADO;;AAEbC,OAAK,GAAG;AACN;;;;;;AAMA;AACA,QAAIC,GAAE,GAAIJ,GAAG,CAAC,CAAD,CAAb;AAEA,QAAIK,GAAE,GAAIL,GAAG,CAAC;AACZM,UAAI,EAAE,KADM;AAEZJ,UAAI,EAAE;AAFM,KAAD,CAAb;AAIA,QAAIK,MAAK,GAAIP,GAAG,CAAC,CACf;AACEM,UAAI,EAAE,KADR;AAEEJ,UAAI,EAAE;AAFR,KADe,CAAD,CAAhB;AAOA,QAAIM,MAAK,GAAIT,QAAQ,CAAC;AACpBU,UAAI,EAAE,MADc;AAEpBC,SAAG,EAAE;AAFe,KAAD,CAArB;;AAIA,aAASC,GAAT,GAAe;AACbP,SAAG,CAACQ,KAAJ;AACAP,SAAG,CAACO,KAAJ,CAAUN,IAAV,GAAiB,KAAjB;AACAD,SAAG,CAACO,KAAJ,CAAUV,IAAV,GAAiB,IAAjB;AAEAW,aAAO,CAACC,GAAR,CAAYT,GAAZ;AACF;;AAEA,aAASU,SAAT,GAAqB;AACnBP,YAAM,CAACE,GAAP,GAAa,EAAb;AACF;;AACA,WAAO;AACLC,SADK;AAELP,SAFK;AAGLC,SAHK;AAILE,YAJK;AAKLC;AALK,KAAP;AAOD;;AA7CY,CAAf","names":["reactive","ref","func","name","setup","num","job","type","jobArr","objArr","work","age","add","value","console","log","changeObj"],"sourceRoot":"","sources":["/Users/liulu/刘璐/vue3/hello-vue3/src/components/SetupHello.vue"],"sourcesContent":["<template>\n  <div>{{ num }}</div>\n  <div>{{ job.type }}</div>\n  <div>{{ job.name }}</div>\n\n  <span v-for=\"(item, index) in jobArr\" :key=\"index\">{{ item.name }}</span>\n  <span>{{ objArr.work }}</span>\n  <button @click=\"add\">num++</button>\n</template>\n\n<script>\nimport { reactive, ref } from \"vue\";\nimport func from \"vue-editor-bridge\";\nexport default {\n  name: \"setup\",\n  setup() {\n    /**\n     * reactive 修改一个响应式对象 不处理基本数据类型\n     * reactive中定义的对象无需.value 可以直接取值\n     * ref 处理基本数据类型\n     * ref中的引用数据类型需要objName.value 取值 实际上ref中的对象处理也是求助了reactive中的proxy\n     */\n    // name job 都是引用对象\n    let num = ref(1);\n\n    let job = ref({\n      type: \"工程师\",\n      name: \"ui设计\",\n    });\n    let jobArr = ref([\n      {\n        type: \"工程师\",\n        name: \"ui设计\",\n      },\n    ]);\n\n    let objArr = reactive({\n      work: \"上班了吗\",\n      age: 12,\n    });\n    function add() {\n      num.value++;\n      job.value.type = \"技术员\";\n      job.value.name = \"电工\";\n\n      console.log(job);\n    }\n\n    function changeObj() {\n      objArr.age = 30;\n    }\n    return {\n      add,\n      num,\n      job,\n      jobArr,\n      objArr,\n    };\n  },\n};\n</script>\n\n<style>\n</style>"]},"metadata":{},"sourceType":"module"}