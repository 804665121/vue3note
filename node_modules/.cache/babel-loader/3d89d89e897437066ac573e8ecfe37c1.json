{"ast":null,"code":"import { reactive, ref } from \"vue\";\nexport default {\n  name: \"setup\",\n\n  setup() {\n    // name job 都是引用对象\n    let num = ref(1);\n    let job = ref({\n      type: \"工程师\",\n      name: \"ui设计\"\n    });\n    let jobArr = ref([{\n      type: \"工程师\",\n      name: \"ui设计\"\n    }]);\n    let objArr = reactive({\n      work: \"上班了吗\"\n    });\n\n    function add() {\n      num.value++;\n      job.value.type = \"技术员\";\n      job.value.name = \"电工\";\n      console.log(job);\n    }\n\n    return {\n      add,\n      num,\n      job,\n      jobArr\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAUA,SAASA,QAAT,EAAmBC,GAAnB,QAA8B,KAA9B;AACA,eAAe;AACbC,MAAI,EAAE,OADO;;AAEbC,OAAK,GAAG;AACN;AACA,QAAIC,GAAE,GAAIH,GAAG,CAAC,CAAD,CAAb;AAEA,QAAII,GAAE,GAAIJ,GAAG,CAAC;AACZK,UAAI,EAAE,KADM;AAEZJ,UAAI,EAAE;AAFM,KAAD,CAAb;AAIA,QAAIK,MAAK,GAAIN,GAAG,CAAC,CACf;AACEK,UAAI,EAAE,KADR;AAEEJ,UAAI,EAAE;AAFR,KADe,CAAD,CAAhB;AAQA,QAAIM,MAAM,GAACR,QAAQ,CAAC;AAClBS,UAAI,EAAC;AADa,KAAD,CAAnB;;AAGA,aAASC,GAAT,GAAe;AACbN,SAAG,CAACO,KAAJ;AAEAN,SAAG,CAACM,KAAJ,CAAUL,IAAV,GAAiB,KAAjB;AACAD,SAAG,CAACM,KAAJ,CAAUT,IAAV,GAAiB,IAAjB;AAEAU,aAAO,CAACC,GAAR,CAAYR,GAAZ;AACF;;AACA,WAAO;AACLK,SADK;AAELN,SAFK;AAGLC,SAHK;AAILE;AAJK,KAAP;AAMD;;AAnCY,CAAf","names":["reactive","ref","name","setup","num","job","type","jobArr","objArr","work","add","value","console","log"],"sourceRoot":"","sources":["/Users/liulu/刘璐/vue3/hello-vue3/src/components/SetupHello.vue"],"sourcesContent":["<template>\n  <div>{{ num }}</div>\n  <div>{{ job.type }}</div>\n  <div>{{ job.name }}</div>\n\n  <span v-for=\"(item, index) in jobArr\" :key=\"index\">{{ item.name }}</span>\n  <button @click=\"add\">num++</button>\n</template>\n\n<script>\nimport { reactive, ref } from \"vue\";\nexport default {\n  name: \"setup\",\n  setup() {\n    // name job 都是引用对象\n    let num = ref(1);\n\n    let job = ref({\n      type: \"工程师\",\n      name: \"ui设计\",\n    });\n    let jobArr = ref([\n      {\n        type: \"工程师\",\n        name: \"ui设计\",\n      },\n    ]);\n\n\n    let objArr=reactive({\n      work:\"上班了吗\"\n    })\n    function add() {\n      num.value++;\n\n      job.value.type = \"技术员\";\n      job.value.name = \"电工\";\n\n      console.log(job);\n    }\n    return {\n      add,\n      num,\n      job,\n      jobArr,\n    };\n  },\n};\n</script>\n\n<style>\n</style>"]},"metadata":{},"sourceType":"module"}