{"ast":null,"code":"import { reactive, ref } from \"vue\";\nimport HelloWorld from \"./HelloWorld.vue\";\nexport default {\n  components: {\n    HelloWorld\n  },\n  name: \"setup\",\n\n  setup() {\n    /**\n     * reactive  处理引用数据类型 修改一个响应式对象  不处理基本数据类型\n     * reactive中定义的对象无需.value 可以直接取值\n     * reactive 可以处理深层次的数据 obj.a.b.c\n     *\n     *\n     * ref 处理基本数据类型\n     * ref中的引用数据类型需要objName.value 取值 实际上ref中的对象处理也是求助了reactive中的proxy\n     */\n    // name job 都是引用对象\n    let p = reactive({\n      job: {\n        type: \"工程师\",\n        name: \"ui设计\"\n      },\n      jobArr: [{\n        type: \"工程师\",\n        name: \"ui设计\"\n      }],\n      objArr: {\n        work: \"上班了吗\",\n        age: 12,\n        a: {\n          b: {\n            c: 89898\n          }\n        }\n      }\n    });\n    let num = ref(1);\n    let {\n      job,\n      objArr,\n      jobArr\n    } = p;\n\n    function add() {\n      num.value++;\n      job.type = \"技术员\";\n      job.name = \"电工\";\n      console.log(job, p);\n    }\n\n    function changeObj() {\n      objArr.age = 30;\n      objArr.a.b.c = \"测试数据\";\n    }\n\n    function setSonVal(val) {\n      console.log(\"helloworld传递过来的值:\" + val);\n    }\n\n    return {\n      num,\n      add,\n      ...p,\n      changeObj,\n      setSonVal\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmBA,SAASA,QAAT,EAAmBC,GAAnB,QAA8B,KAA9B;AACA,OAAOC,UAAP,MAAuB,kBAAvB;AACA,eAAe;AACbC,YAAU,EAAE;AAAED;AAAF,GADC;AAEbE,MAAI,EAAE,OAFO;;AAGbC,OAAK,GAAG;AACN;;;;;;;;;AASA;AAEA,QAAIC,IAAIN,QAAQ,CAAC;AACfO,SAAG,EAAE;AACHC,YAAI,EAAE,KADH;AAEHJ,YAAI,EAAE;AAFH,OADU;AAKfK,YAAM,EAAE,CACN;AACED,YAAI,EAAE,KADR;AAEEJ,YAAI,EAAE;AAFR,OADM,CALO;AAWfM,YAAM,EAAE;AACNC,YAAI,EAAE,MADA;AAENC,WAAG,EAAE,EAFC;AAGNC,SAAC,EAAE;AACDC,WAAC,EAAE;AACDC,aAAC,EAAE;AADF;AADF;AAHG;AAXO,KAAD,CAAhB;AAqBA,QAAIC,GAAE,GAAIf,GAAG,CAAC,CAAD,CAAb;AACA,QAAI;AAAEM,SAAF;AAAOG,YAAP;AAAeD;AAAf,QAA0BH,CAA9B;;AACA,aAASW,GAAT,GAAe;AACbD,SAAG,CAACE,KAAJ;AACAX,SAAG,CAACC,IAAJ,GAAW,KAAX;AACAD,SAAG,CAACH,IAAJ,GAAW,IAAX;AACAe,aAAO,CAACC,GAAR,CAAYb,GAAZ,EAAiBD,CAAjB;AACF;;AACA,aAASe,SAAT,GAAqB;AACnBX,YAAM,CAACE,GAAP,GAAa,EAAb;AACAF,YAAM,CAACG,CAAP,CAASC,CAAT,CAAWC,CAAX,GAAe,MAAf;AACF;;AAEA,aAASO,SAAT,CAAmBC,GAAnB,EAAwB;AACtBJ,aAAO,CAACC,GAAR,CAAY,sBAAsBG,GAAlC;AACF;;AACA,WAAO;AACLP,SADK;AAELC,SAFK;AAGL,SAAGX,CAHE;AAILe,eAJK;AAKLC;AALK,KAAP;AAOD;;AA3DY,CAAf","names":["reactive","ref","HelloWorld","components","name","setup","p","job","type","jobArr","objArr","work","age","a","b","c","num","add","value","console","log","changeObj","setSonVal","val"],"sourceRoot":"","sources":["/Users/liulu/刘璐/vue3/hello-vue3/src/components/SetupHello.vue"],"sourcesContent":["<template>\n  <h1>初识Setup</h1>\n  <div>{{ num }}</div>\n  <div>{{ job.type }}</div>\n  <div>{{ job.name }}</div>\n  <span v-for=\"(item, index) in jobArr\" :key=\"index\">{{ item.name }}</span>\n  <p>{{ objArr.work }}</p>\n  <p>{{ objArr.age }}</p>\n  <p>{{ objArr.a.b.c }}</p>\n  <button @click=\"add\">num++</button>\n  <button @click=\"changeObj\">修改对象中的值</button>\n  <hello-world msg=\"原理信息\" info=\"hello\" @hello=\"setSonVal\">\n    <template v-slot:info>\n      <span>这是父组件传递过来的值</span>\n    </template>\n  </hello-world>\n</template>\n\n<script>\nimport { reactive, ref } from \"vue\";\nimport HelloWorld from \"./HelloWorld.vue\";\nexport default {\n  components: { HelloWorld },\n  name: \"setup\",\n  setup() {\n    /**\n     * reactive  处理引用数据类型 修改一个响应式对象  不处理基本数据类型\n     * reactive中定义的对象无需.value 可以直接取值\n     * reactive 可以处理深层次的数据 obj.a.b.c\n     *\n     *\n     * ref 处理基本数据类型\n     * ref中的引用数据类型需要objName.value 取值 实际上ref中的对象处理也是求助了reactive中的proxy\n     */\n    // name job 都是引用对象\n\n    let p = reactive({\n      job: {\n        type: \"工程师\",\n        name: \"ui设计\",\n      },\n      jobArr: [\n        {\n          type: \"工程师\",\n          name: \"ui设计\",\n        },\n      ],\n      objArr: {\n        work: \"上班了吗\",\n        age: 12,\n        a: {\n          b: {\n            c: 89898,\n          },\n        },\n      },\n    });\n    let num = ref(1);\n    let { job, objArr, jobArr } = p;\n    function add() {\n      num.value++;\n      job.type = \"技术员\";\n      job.name = \"电工\";\n      console.log(job, p);\n    }\n    function changeObj() {\n      objArr.age = 30;\n      objArr.a.b.c = \"测试数据\";\n    }\n\n    function setSonVal(val) {\n      console.log(\"helloworld传递过来的值:\" + val);\n    }\n    return {\n      num,\n      add,\n      ...p,\n      changeObj,\n      setSonVal,\n    };\n  },\n};\n</script>\n\n<style>\n</style>"]},"metadata":{},"sourceType":"module"}