{"ast":null,"code":"import { reactive, computed } from \"@vue/reactivity\";\nexport default {\n  name: \"ChangeUser\",\n\n  setup() {\n    let person = reactive({\n      firstName: \"\",\n      lastName: \"\"\n    }); // 计算属性 简写 未考虑修改情况\n    //  person.fullName = computed(() => {\n    //   return person.firstName + person.lastName;\n    // });\n    //   计算属性完整形式\n\n    person.fullName = computed({\n      get() {\n        return person.firstName + \"-\" + person.lastName;\n      },\n\n      set(value) {\n        const name = value.split(\"-\");\n        person.firstName = name[0];\n        person.lastName = name[1];\n      }\n\n    });\n    return {\n      person\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAWA,SAASA,QAAT,EAAmBC,QAAnB,QAAmC,iBAAnC;AACA,eAAe;AACbC,MAAI,EAAE,YADO;;AAEbC,OAAK,GAAG;AACN,QAAIC,MAAK,GAAIJ,QAAQ,CAAC;AACpBK,eAAS,EAAE,EADS;AAEpBC,cAAQ,EAAE;AAFU,KAAD,CAArB,CADM,CAMN;AACA;AACA;AACA;AAGJ;;AACIF,UAAM,CAACG,QAAP,GAAkBN,QAAQ,CAAC;AACzBO,SAAG,GAAG;AACJ,eAAOJ,MAAM,CAACC,SAAP,GAAmB,GAAnB,GAAyBD,MAAM,CAACE,QAAvC;AACD,OAHwB;;AAIzBG,SAAG,CAACC,KAAD,EAAQ;AACT,cAAMR,IAAG,GAAIQ,KAAK,CAACC,KAAN,CAAY,GAAZ,CAAb;AACAP,cAAM,CAACC,SAAP,GAAmBH,IAAI,CAAC,CAAD,CAAvB;AACAE,cAAM,CAACE,QAAP,GAAkBJ,IAAI,CAAC,CAAD,CAAtB;AACD;;AARwB,KAAD,CAA1B;AAWA,WAAO;AACLE;AADK,KAAP;AAGD;;AA7BY,CAAf","names":["reactive","computed","name","setup","person","firstName","lastName","fullName","get","set","value","split"],"sourceRoot":"","sources":["/Users/liulu/刘璐/vue3/hello-vue3/src/components/ChangeUser.vue"],"sourcesContent":["<template>\n  <div><h1>计算属性的学习</h1></div>\n\n  <div>姓: <input v-model=\"person.firstName\" /></div>\n  <div>名: <input v-model=\"person.lastName\" /></div>\n  <p>{{ person.fullName }}</p>\n\n  <div>全名: <input v-model=\"person.fullName\" /></div>\n</template>\n\n<script>\nimport { reactive, computed } from \"@vue/reactivity\";\nexport default {\n  name: \"ChangeUser\",\n  setup() {\n    let person = reactive({\n      firstName: \"\",\n      lastName: \"\",\n    });\n\n    // 计算属性 简写 未考虑修改情况\n    //  person.fullName = computed(() => {\n    //   return person.firstName + person.lastName;\n    // });\n  \n\n//   计算属性完整形式\n    person.fullName = computed({\n      get() {\n        return person.firstName + \"-\" + person.lastName;\n      },\n      set(value) {\n        const name = value.split(\"-\");\n        person.firstName = name[0];\n        person.lastName = name[1];\n      },\n    });\n\n    return {\n      person,\n    };\n  },\n};\n</script>\n\n<style>\n</style>"]},"metadata":{},"sourceType":"module"}